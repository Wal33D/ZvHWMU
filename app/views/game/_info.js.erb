  <div id="info">

<%= search_form_for @q, :url => profile_path, :html => { :method => :get, :class => "well form-inline" } do |f| %>
  <div class="form-group">
    <%= f.text_field :name_cont, :type => "search", :class => "form-control", :placeholder => "Search by name" %>
    <br>
    <%= f.text_field :email_cont, :type => "search", :class => "form-control", :placeholder => "Search by email" %>
  </div>
  <%= f.submit "Search", :class => "btn btn-default"
%>
<% end %>

<table class="sortable table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th><%= sortable "name", "Name" %></th>
      <th><%= sortable "email" %></th>

    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr class="user">
        <td><%= link_to(user.name, admin_user_path(user)) %></td>
        <td><%= link_to(user.email, admin_user_path(user)) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @users %>

</div>