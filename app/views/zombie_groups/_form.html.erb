<%= simple_form_for(@zombie_group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :address, :required => true %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
