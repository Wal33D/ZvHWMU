<div>
<div id="map" >
    <div id="multi_overlays" ></div>
</div>
</div>
<style>
    #map{
    	position:absolute !important;
    	top:0;
    	bottom:0;
    	right:0;
    	left:0;
    	padding-top:60px;

    	margin-right:auto;
    	margin-left:auto;
    	margin-top: 5px;
    	width: 635px   !important;
    	height:100%  !important;

    }
    #multi_overlays{
width: 100%; height: 100%;
   	margin-left:auto  !important;

    }
        #map-canvas img {
            max-width: none;

    }
    }
</style>
<script>
    $('#map').collapse({
        toggle: true
    });
    var handler = Gmaps.build('Google');

    handler.buildMap({
        internal: {
            id: 'multi_overlays'
        }
    }, function() {


        var circles = handler.addCircles(
            [{
                lat: 42.254133,
                lng: -85.640692,
                radius: 40
            }], {
                strokeColor: '#FF0000'
            }

        );

        handler.bounds.extendWith(circles);
         var circles = handler.addCircles(
            [{
                lat: 42.255133,
                lng: -85.641692,
                radius: 40
            }], {
                strokeColor: '#FF0000'
            }
            
        );

        handler.bounds.extendWith(circles);
        handler.fitMapToBounds();
    });
</script>